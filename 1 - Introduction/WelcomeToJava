public class WelcomeToJava {

    public static void main (String[] args){

        String palabraUno = "World";
        String palabraDos = "Java";
        String saludo = "Hello, %s";
        String saludoPrintUno = String.format(saludo,palabraUno);
        String saludoPrintDos = String.format(saludo,palabraDos);

        System.out.println(saludoPrintUno);
        System.out.println(saludoPrintDos);

    }

}
