import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class JavaEndOfFile {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int numLinea = 1;

        while (scan.hasNext()==true){
            String texto = scan.nextLine();
            System.out.println(numLinea + " " + texto);
            numLinea++;
        }
    }
}